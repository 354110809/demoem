/* pages/mine/index.wxss */
.tabbar4Page{
   min-height:calc( 100vh - 48px);
   display: flex;
   flex-direction: column;
}
.top-box{
   padding:20px;
   flex:none;
}
.t-box1{
   padding-bottom: 36px;
   display: flex;
   align-items: center;
   margin-right: 16px;
   position: relative;
   width:100%;
}
.t-box1 .image{
   width:100px;
   height:100px;
   border-radius: 25px;
   margin-right: 16px;
}
.t-box1 .t-box-r{
   flex-grow: 1;
}
.t-box1 .t-box-r .name{
   font-weight: 600;
   font-size: 15pt;
   color: #111111;
   margin-bottom: 7px;
   max-width: 8em;
}
.t-box1 .t-box-r .zl{
   font-size: 9pt;
   color: #6F6F6F;
   display: inline-block;
   padding-right:20px;
   background:url(data:image/jpeg;base64,) no-repeat center right / auto 100%;
}
.t-box1 .t-box-rt {
   padding:8px 14px 8px 17px;
   background: #FDF8F8;
   border-radius: 20px 0px 0px 20px;
   position: absolute;
   top:0;
   right:-20px;
   display: flex;
   align-items: center;
   justify-content: center;
}
.t-box1 .t-box-rt  .vector{
      font-weight: 600;
      color:#FC485D;
      font-size: 10.5pt;
      padding-left:22px;
      background:url(data:image/png;base64,) no-repeat left center/auto 80%;
}

.t-box2{
   display: flex;
   padding-bottom: 18px;
}
.t-box2>view{
   display: flex;
   margin-right: 18px;
   align-items: center;
}
.t-box2>view .image{
   width:48px;
   height:48px;
   flex:none;
   margin-right: 12px;
}
.t-box2>view .t-box2-r{
   flex-grow: 1;
}
.t-box2>view .t-box2-r .t-box2-r-1{
   font-weight: 600;
   font-size: 12pt;
   color: #111111;
   margin-bottom: 2px;
}
.t-box2>view .t-box2-r .t-box2-r-2{
   font-size: 9pt;
   color: #6F6F6F;
}

.t-box3{
   margin:0 3px;
   height:57px;
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   border-radius: 16px;
   color:#fff;
   display: flex;
   align-items: center;
   justify-content:space-around;
}
.t-box3 .t-box3-1,.t-box3 .t-box3-2{
   font-size: 12pt;
}
.t-box3 view view:nth-child(1) text{
   font-size: 11pt;
}
.t-box3 view view:nth-child(2){
   padding-top: 3px;
   font-size: 9pt;
   color:rgba(255, 255, 255, 0.6);
}
.t-box3 .t-box3-3{
   width: 80px;
   height: 30px;
   display: flex;
   align-items: center;
   justify-content: center;
   background: #FFFFFF;
   border-radius: 72px;
   font-weight: 500;
   font-size: 10.5pt;
}
.t-box3 .t-box3-3 text{
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   -webkit-background-clip: text;

   color: transparent;
}


.bottom-box{
   background: #F0F4F7;
   flex-grow: 1;
}
.bottom-box .bottom-head{
   padding:3px 30px 0;
   height:50px;
   display: flex;
   justify-content:space-between;
   position:sticky;
   top:0;
   left:0;
   z-index: 1;
   background: #F0F4F7;
}
.bottom-box .bottom-head>view{
   height:100%;
   display: flex;
   justify-content: center;
   align-items: center;
   font-weight: 600;
   font-size: 10.5pt;
   color: #BFBFBF;
}
.bottom-box .bottom-head>view.cur{
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   -webkit-background-clip: text;

   color: transparent;
}
.bottom-box .bottom-body{
   padding:0 12px 12px;
}
.bottom-box .bottom-body .bottom-body-inner{
   background:#fff;
   border-radius: 8px;
   min-height: 200px;
}

.bottom-body-inner .item{
   display: flex;
   padding:15px 10px;
   border-bottom:1px solid #F0F4F7;
   position: relative;
}
.bottom-body-inner .item:last-child{
   border-bottom:none;
}
.bottom-body-inner1 .item .pic{
   flex:none;
   position: relative;
   width:36px;
   padding-bottom: 10px;
   margin-right: 9px;
}
.bottom-body-inner1 .item .pic .image{
   border-radius: 50%;
   width:36px;
   height:36px;
   display: block;
}
.bottom-body-inner1 .item .pic view{
   font-size: 8pt;
   text-align: center;
   display: block;
   padding:0px 3px;
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   border-radius: 20px;
   color:#fff;
   width:100%;
   position: absolute;
   bottom:7px;
   left: 50%;
   transform: translate(-50%,0);
}


.bottom-body-inner1 .item .info{
   width:calc( 100% - 45px );
   display: flex;
   flex-direction: column;
   justify-content:space-around;
   padding-right: 40px;
   box-sizing: border-box;
}
.bottom-body-inner1 .item .info .line-name{
   flex-grow: 1;
   display: flex;
   align-items: flex-end;
   color:#232832;
   font-weight: 500;
   margin-bottom: 10px;
}
.bottom-body-inner1 .item .info .line-name .name{
   font-size: 11.25pt;
   overflow: hidden;
   white-space: nowrap;
   /*不换行*/
   text-overflow: ellipsis;
}
.bottom-body-inner1 .item .info .line-name .age{
   font-size: 9pt;
   margin:0 5px;
   flex: none;
}
.bottom-body-inner1 .item .info .line-name .vector{
   font-weight: 600;
   color:#FC485D;
   font-size: 10.5pt;
   padding-left:22px;
   background:url(data:image/png;base64,) no-repeat left center/auto 80%;
}
.bottom-body-inner1 .item .info .text{
   font-size: 10.5pt;
   color:#111111;
   overflow: hidden;
	text-overflow: ellipsis;	
   white-space: nowrap; 
}
.bottom-body-inner1 .item .status{
   width:40px;
   position: absolute;
   top:10px;
   right:10px;
   display: flex;
   align-items: center;
   justify-content: center;
   flex-direction: column;
   font-size: 9pt;
   color: #BFBFBF;
   z-index: 0;

}
.bottom-body-inner1 .item .tu{
   width:28px;
   height:28px;
}
.bottom-body-inner1 .item .cur1{
   background:url(data:image/jpeg;base64,) no-repeat center / 100% 100%;
}
.bottom-body-inner1 .item .cur2{
   background:url(data:image/jpeg;base64,) no-repeat center/100%;
}
.bottom-body-inner1 .item .cur3{
   background:url(data:image/jpeg;base64,) no-repeat center / 100%;
}

.bottom-body-inner2 .item{

}
.bottom-body-inner2 .item .image{
   width:21vw;
   height:21vw;
   border-radius: 4px;
   flex:none;
   margin-right: 12px;
}
.bottom-body-inner2 .item .item_body_t_i{
   flex-grow: 1;
   color:#BFBFBF;
   font-size: 9pt;
}
.bottom-body-inner2 .item .item_body_t_i .name{
   font-weight: 500;
   font-size: 10.5pt;
   color:#111;
   display: -webkit-box; /*重点，不能用block等其他，将对象作为弹性伸缩盒子模型显示*/
   -webkit-box-orient: vertical; /*从上到下垂直排列子元素（设置伸缩盒子的子元素排列方式）*/
   -webkit-line-clamp: 2; /*行数，超出三行隐藏且多余的用省略号表示...*/
   line-clamp: 2;
   word-break: break-all;
   overflow: hidden;
   margin-bottom: 5px;
}
.bottom-body-inner2 .item .item_body_t_i .date,
.bottom-body-inner2 .item .item_body_t_i .address{
   margin-bottom: 5px;
   display: -webkit-box; /*重点，不能用block等其他，将对象作为弹性伸缩盒子模型显示*/
   -webkit-box-orient: vertical; /*从上到下垂直排列子元素（设置伸缩盒子的子元素排列方式）*/
   -webkit-line-clamp: 2; /*行数，超出三行隐藏且多余的用省略号表示...*/
   line-clamp: 2;
   word-break: break-all;
   overflow: hidden;
}
.bottom-body-inner2 .item .item_body_t_i .date text,
.bottom-body-inner2 .item .item_body_t_i .address text{
   color:#111;
}
.bottom-body-inner2 .item .item_body_t_i .myactivity{
   background: #FEF1F7;
   border-radius: 2px;
   font-size: 9pt;
   color: #FB605F;
   padding:2px 5px;
   display: inline-block;
}

.bottom-body-inner3 .item .image{
   width:36px;
   height:36px;
   border-radius: 50%;
   flex:none;
   margin-right: 12px;
}
.bottom-body-inner3 .item .item-right{
   flex-grow: 1;
}
.bottom-body-inner3 .item .item-right .item-right-1{
   /* display: flex; */
   /* align-items: flex-end; */
   font-size: 11.25pt;
   color: #232832;
   margin-bottom: 6px;
   overflow: hidden;
}
.bottom-body-inner3 .item .item-right .item-right-1 text{
   font-size: 9pt;
   margin-left: 8px;
   /* flex:none; */
}
.bottom-body-inner3 .item .item-right .item-right-2{
   color: #666666;
   font-size: 9pt;
   margin-bottom: 12px;
}
.bottom-body-inner3 .item .item-right .item-right-3{
   color: #BFBFBF;
   font-size: 9pt;
   margin-bottom: 12px;
   display: flex;
   align-items: center;
}
.bottom-body-inner3 .item .item-right .item-right-3 text{
   margin-right: 10px;
}
.bottom-body-inner3 .item .item-right .item-right-4{
   white-space: nowrap;
   overflow-x: auto;
   margin-bottom: 12px;
}
.bottom-body-inner3 .item .item-right .item-right-4 text{
   display: inline-block;
   background: rgba(252, 221, 236, 0.4);
   border-radius: 2px;
   padding: 3px 6px;
   color:#FB605F;
   font-size:8pt;
   margin-right: 8px;
}
.bottom-body-inner3 .item .item-right .item-right-4 text:last-child{
   margin-right: 0;
}


.bottom-body-inner3 .item .item-right .item-right-5{
   background: #FDF8F8;
   border-radius: 6px;
   padding:6px;
   color:#333333;
   font-size: 10.5pt;
}