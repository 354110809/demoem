/* tabbarPages/corner/index.wxss */
.inner .item{
   margin-bottom: 12px;
   background: #fff;
   border-radius: 8px;
   box-sizing: border-box;
   padding:12px 0;
}
.item .groupInfo{
   padding:0 12px 12px;
   display: flex;
   align-items: center;
   border-bottom: 2px solid #eff4f7;
}
.item .groupInfo .group-pic{
   flex:none;
   position: relative;
   width:40px;
   padding-bottom: 10px;
   text-align: center;
}
.item .groupInfo .group-pic image{
   width: 40px;
   height:40px;
   display: block;
   border-radius: 50%;
}
.item .groupInfo .group-pic view{
   font-size: 8pt;
   display: block;
   padding:1px 3px;
   background: linear-gradient(266.37deg, #C98661 -14.53%, #BF9767 113.67%);
   border-radius: 2px;
   color:#fff;
   width:68%;
   position: absolute;
   bottom:0;
   left: 50%;
   transform: translate(-50%,0);
}

.item .groupInfo .group-text{
   display: flex;
   padding-left: 8px;
   flex-direction: column;
   justify-content:space-around;
}
.item .groupInfo .group-text-name{
   display: flex;
   font-size:12pt;
   font-weight: 600;
   color:#111;
   align-items: flex-end;
   margin-bottom: 5px;
}
.item .groupInfo .group-text-name view{
   overflow: hidden;
   max-width: 7em;
   white-space: nowrap;
   /*不换行*/
   text-overflow: ellipsis;
}
.item .groupInfo .group-text-name text{
   font-size:9pt;
   margin-left: 8px;
}
.item .groupInfo .praiseDeg{
   color:#BFBFBF;
   font-size: 9pt;
   display: flex;
   align-items: center;
}
.item .groupInfo .praiseDeg text{
   margin-right: 12px;
}
.item .groupInfo .group-btn{
   margin-left: auto;
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   border-radius: 72px;
   width:80px;
   height:30px;
   display: flex;
   align-items: center;
   justify-content: center;
   color:#fff;
   font-size: 10.5pt;
}
.item .groupInfo .group-btn.cur{
   color:#999999;
   background:#E6E8EA;
}

.item .groupMember{
   padding:12px 0;
   display: flex;
   flex-wrap: wrap;
}
.item .groupMember .group-member-info{
   width:25%;
   box-sizing: border-box;
   padding:0 12px;
   flex:none;
   box-sizing: border-box;
   display: flex;
   flex-direction: column;
   align-items: center;
   margin-bottom: 20px;
   color:#666666;
   font-size: 8pt;
}
.item .groupMember .group-member-info image{
   width:48px;
   height:48px;
   border-radius: 50%;
   display: block;
   margin-bottom: 3px;
}
.item .groupMember .group-member-info .infoText{
   overflow: hidden;
   width:100%;
   white-space: nowrap;
   /*不换行*/
   text-overflow: ellipsis;
   padding-top: 3px;
}

.item .groupMember .group-member-info:nth-last-of-type(1),
.item .groupMember .group-member-info:nth-last-of-type(2),
.item .groupMember .group-member-info:nth-last-of-type(3),
.item .groupMember .group-member-info:nth-last-of-type(4){
   margin-bottom: 0;
}

.item .groupBtn{
   width:100%;
   height:20px;
   display: flex;
   align-items: center;
   justify-content: center;
}
.item .groupBtn .btn{
   width:14px;
   height:14px;
   transform-origin: center;
   transform: rotate(90deg);
   transition: all 0.1s 0;
   background: url(data:image/jpeg;base64,) no-repeat center / 14px 14px;
}
.item .groupBtn .btn.cur{
   transform: rotate(-90deg);
}