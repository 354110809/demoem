.activityBanner{
    width:100%;
    border-radius:6px;
    overflow: hidden;
    position: relative;
}
.activityBanner image{
    width:100%;
    height:48vw;
    display: block;
}
.activityBanner .nameActivity{
    position: absolute;
    left: 0;
    bottom:0 ;
    width: 100%;
    height: 45px;
    background: rgba(0, 0, 0, 0.4);
    font-weight: 500;
    font-size: 14px;
    color: #FFFFFF;
    line-height: 45px;
    overflow: hidden;
    /*超出部分隐藏*/
    white-space: nowrap;
    /*不换行*/
    text-overflow: ellipsis;
}


.title-box{
    font-family: PingFang SC;
    font-weight: 400;
    font-size: 11.75pt;
    line-height: 18px;
    display: flex;
    color: #111111;
    padding:16px 8px;
}
.title-box text{
    font-weight: 600;
}
.platformUserList{
    display: flex;
    flex-wrap: wrap;
    justify-content:space-between;
}
.platformUserList .item{
    display: flex;
    flex-direction: column;
    width:49%;
    height:64.53vw;
    background:#fff;
    margin-bottom:8px;
    border-radius: 8px;
    overflow: hidden;
}
.platformUserList .item .top-pic{
    position: relative;
    width:100%;
    flex: none;
}
.platformUserList .item .top-pic image{
    display: block;
    width:100%;
    height:48.53vw;
}
.platformUserList .item .top-pic .top-pic-info{
    position: absolute;
    top:8px;
    left:0;
    z-index: 1;
    width:100%;
    height:18px;
    padding-left: 8px;
    box-sizing: border-box;
    display: flex;
}
.platformUserList .item .top-pic .vector{
    font-weight: bold;
    color:#FC485D;
    text-shadow: 0 0 1px #fff,0 0 1px #fff,0 0 1px #fff,0 0 1px #fff,0 0 1px #fff,0 0 1px #fff;  
    font-size: 10.5pt;
    padding-left:22px;
    background:url(data:image/png;base64,) no-repeat left center/auto 100%;
}
.platformUserList .item .top-pic .make{
    background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
    border-radius: 72px;
    margin: 0px 6px;
    color:#fff;
    font-size: 9pt;
    padding:0 5px;
    height:100%;
    display: flex;
    align-items: center;
}

.platformUserList .item .bottom-info{
    position: relative;
    flex:none;
    display: flex;
    align-items: flex-end;
    height:16vw;
    
}
.platformUserList .item .bottom-info .inner{
    width:100%;
    height:21.33vw;
    background: rgba(255, 255, 255, 0.8);
    backdrop-filter: blur(4px);
    border-radius: 8px 8px 0px 0px;
    border-radius: 8px;
    box-sizing: border-box;
    padding:6px 8px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.platformUserList .item .bottom-info .inner .infoLine{
    display: flex;
    align-items: flex-end;
    color: #232832;
}
.platformUserList .item .bottom-info .inner .infoLine .name{
    font-family: PingFang SC;
    font-weight: 500;
    font-size: 11.25pt;
    flex: none;
    width:5em;
    overflow: hidden;
    white-space: nowrap;
    /*不换行*/
    text-overflow: ellipsis;
    margin-right: 5px;
}
.platformUserList .item .bottom-info .inner .infoLine .ageGender{
    font-size: 9pt;
}
.platformUserList .item .bottom-info .inner .label{
    white-space: nowrap;
    overflow-x: auto;
}
.platformUserList .item .bottom-info .inner .label text{
    display: inline-block;
    background: rgba(252, 221, 236, 0.4);
    border-radius: 2px;
    padding: 3px 6px;
    color:#FB605F;
    font-size:8pt;
    margin-right: 8px;
}
.platformUserList .item .bottom-info .inner .label text:last-child{
    margin-right: 0;
}
.platformUserList .item .bottom-info .inner .place{
    padding-left:15px;
    color:#999;
    font-size:9pt;
    height:18px;
    line-height: 18px;
    overflow: hidden;
    white-space: nowrap;
    /*不换行*/
    text-overflow: ellipsis;
    background:url(data:image/png;base64,) no-repeat left center/auto 14px;
}

