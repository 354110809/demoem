/* tabbarPages/activity/index.wxss */
/* 活动付款提示 */
.tips_model{
   background: #ffffff;
   padding:10px 18px;
   position:sticky;
   top:54px;
   left:0;
   box-shadow: 0 1px 2px #eff4f7;
}
.tips_model .inner{
   background: #FFEEDE;
   border-radius: 4px;
   padding:10px 12px;
   height:24px;
   display: flex;
   align-items: center;
   font-size:10.5pt;
   color:#FB605F;
}
.tips_model .inner .icon{
   width:24px;
   height:24px;
   margin-right: 6px;
   background:url(data:image/jpeg;base64,) no-repeat center/100%;
   flex:none;
}
.tips_model .inner .btn{
   flex:none;
   background:#EB5757;
   color: #FFFFFF;
   height:24px;
   padding:0 12px;
   border-radius: 12px;
   display: flex;
   align-items: center;
   margin-left: auto;
   font-size:9pt;
}

.tabbar3Page .body.cur{
   min-height: calc( 100vh - 182px);
}

.activity .item{
   margin-bottom: 12px;
   background: #fff;
   border-radius: 8px;
   overflow: hidden;
}
.activity .item .item_head{
   padding:12px 0 8px;
   display: flex;
   align-items: center;
}
.activity .item .item_head .dateStr{
   color:#F92B29;
   font-size: 12pt;
   font-weight: 600;
   padding-left:12px;
   position: relative;
}
.activity .item .item_head .dateStr::after{
   position: absolute;
   top:50%;
   left:0;
   transform: translate(0,-50%);
   content: '';
   width:4px;
   height:16px;
   background:#F92B29;

}
.activity .item .item_head .date{
   color:#F92B29;
   font-size: 9pt;
   font-weight: 600;
}
.activity .item .item_head  .type{
   color:#BFBFBF;
   font-size: 9pt;
}
.activity .item .item_body{
   padding:12px;
   padding-top: 4px;
}
.item_body .item_body_t{
   display: flex;
   padding-bottom:12px;
}
.item_body .item_body_t .image{
   width:21vw;
   height:21vw;
   border-radius: 4px;
   flex:none;
   margin-right: 12px;
}
.item_body .item_body_t .item_body_t_i{
   flex-grow: 1;
   color:#BFBFBF;
   font-size: 9pt;
}
.item_body .item_body_t .item_body_t_i .name{
   font-weight: 500;
   font-size: 10.5pt;
   color:#111;
}
.item_body .item_body_t .item_body_t_i .date,
.item_body .item_body_t .item_body_t_i .address{
   padding-top: 5px;
}
.item_body .item_body_t .item_body_t_i .date text,
.item_body .item_body_t .item_body_t_i .address text{
   color:#111;
}
.item_body .item_body_b{
   border-top:1px solid #E8E8E8;
   display: flex;
   align-items: center;
   padding-top: 12px;
}
.item_body .item_body_b .item_body_b_li{
   margin-right: 12px;
   flex:none;
}
.item_body .item_body_b .item_body_b_li .text{
   color:#999999;
   font-size: 9pt;
   margin-bottom: 6px;
}
.item_body .item_body_b .item_body_b_li .info{
   display: flex;
   align-items: center;
   height:24px;
}
.item_body_b_li .bm{
   font-size: 12pt;
   color:#111;
   font-weight: 600;
}
.item_body_b_li .zzz{
   flex:none;
}
.item_body_b_li .zzz .image{
   width:24px;
   height:24px;
   border-radius: 50%;
}
.item_body_b_li .zzz view{
   font-size: 9pt;
   color:#111;
   max-width: 4em;
   overflow: hidden;
   white-space: nowrap;
    /*不换行*/
    text-overflow: ellipsis;
    padding-left: 4px;
}
.item_body_b_li .cy{
   padding-right:20px;
   background: url(data:image/jpeg;base64,) no-repeat center right / 14px 14px;
}
.item_body_b_li .cy .image{
   width:24px;
   height:24px;
   border-radius: 50%;
   margin-right: 6px;
}

.activity .item .item_body .item_body_b .item_body_b_li:last-child{
   margin-right: 0;
   flex-grow: 1;
}
/* 活动按钮 */
.activity .item .item_foter{
   display: flex;
   justify-content: flex-end;
   padding:6px 12px 12px;
}
.activity .item .item_foter .btn{
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   width:80px;
   height:30px;
   font-size:10.5pt;
   display: flex;
   align-items: center;
   justify-content: center;
   color:#fff;
   border-radius: 15px;
}
.activity .item .item_foter .btn.see{
   background:#E6E8EA;
   color:#999999;
}
/* 发布活动按钮 */
.release-btn{
   position: fixed;
   bottom: 65px;
   left:50%;
   width: 145px;
   height: 44px;
   transform: translate(-50%,0);
   background: linear-gradient(266.37deg, #FB3A74 -14.53%, #FF6230 113.67%);
   box-shadow: 0px 0px 7px rgba(255, 13, 13, 0.25);
   border-radius: 72px;
   color:#fff;
   display: flex;
   align-items: center;
   justify-content: center;
   font-size: 10.5pt;
}
